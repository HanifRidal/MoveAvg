<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary mb-2">
      Pengeluaran Pakan DataTable
    </h6>
    <button
      class="btn bg-success btn-circle btn-sm"
      data-toggle="modal"
      data-target="#exampleModal"
    >
      <i class="fas fa-plus text-gray-100"></i>
    </button>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="0"
      >
        <thead>
          <tr>
            <th>No</th>
            <th>Tanggal</th>
            <th>Pakan Harian /Kg</th>
            <th>Harga Pakan</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>1</td>
            <td>bla</td>
            <td>bla</td>
            <td>bla</td>
            <td>
              <a
                class="btn bg-warning btn-circle btn-sm button-update"
                href="javascript:void(0)"
                data-toggle="modal"
                type="button"
                title="Update"
              >
                <i class="fas fa-edit text-gray-100"></i>
              </a>
              <a
                class="btn bg-danger btn-circle btn-sm"
                href="/admin/telur/delete/id"
                title="Delete"
              >
                <i class="fas fa-trash text-gray-100"></i>
              </a>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
