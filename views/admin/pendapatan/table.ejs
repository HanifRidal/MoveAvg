    <h6 class="m-0 font-weight-bold text-primary">Pendapatan DataTable</h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="0"
      >
        <thead>
          <tr>
            <th>No</th>
            <th>Create At</th>
            <th>Per Bulan</th>
            <th>Per Quartal</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>1</td>
            <td>bla</td>
            <td>bla</td>
            <td>bla</td>
            <td>
              <a
                class="btn bg-warning btn-circle btn-sm"
                href="/admin/telur/update/id"
                title="Update"
              >
                <i class="fas fa-edit text-gray-100"></i>
              </a>
              <a
                class="btn bg-danger btn-circle btn-sm"
                href="/admin/telur/delete/id"
                title="Delete"
              >
                <i class="fas fa-trash text-gray-100"></i>
              </a>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
